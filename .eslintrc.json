{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "globals": {
        "window": true,
        "module": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["warn", "always"],
        "quotes": ["warn", "single"],
        "prefer-const": "warn",
        "indent": ["warn", 2],
        "react/jsx-key": "warn",
        "no-unused-vars": ["warn", {"vars": "all"}]
    }
}
